program Animation;

uses
  Forms,
  UMain in 'UMain.pas' {Fmain},
  umessage in 'umessage.pas' {Fmsg};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(TFmain, Fmain);
  Application.CreateForm(TFmsg, Fmsg);
  Application.Run;
end.
